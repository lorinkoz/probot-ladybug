{
  "name": "probot-ladybug",
  "version": "0.2.0",
  "private": true,
  "description": "Probot App to triage issues",
  "author": "Lorenzo Pe√±a <lorinkoz@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/lorinkoz/probot-ladybug.git",
  "homepage": "https://github.com/lorinkoz/probot-ladybug",
  "bugs": "https://github.com/lorinkoz/probot-ladybug/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "probot": "^9.5.3",
    "probot-commands": "^1.1.0",
    "probot-metadata": "^1.0.1",
    "probot-scheduler": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.1.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^25.0.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
